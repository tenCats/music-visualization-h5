<!DOCTYPE html>
<html lang='zh-CN'>
  <head>
    <meta name='viewport' content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no'>
    <title><%= title %></title>

    <!-- bootstrap -->
    <link rel='stylesheet' href='/stylesheets/bootstrap.min.css' />
    <!-- my own stylesheets -->
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <div class='container mycontainer'>
      <div class='player'>
        <div class='backimage'></div>
        <div class='circle painter'>
          <canvas class='display canvas' id='canvas'>
          </canvas>
          <div class='display' id='cover'>
            <img class='cd-cover' src='images/0.jpg'/>
          </div>
        </div>
        <div class='circle outline'></div>
      </div>
      <div class='info'>
        <span id='ti'><%= ti %></span>
        <span id='ar'><%= ar %></span><br>
        <span id='wl'><%= wl %></span>
        <span id='wm'><%= wm %></span>
      </div>
<!--
      <div class='settings'>
        <div class='seting-head'>
          <div class='setting-icon'>
            <span class="glyphicon glyphicon-cog" aria-hidden="true"></span>
          </div> 
        </div>
        <div class='setting-body'>
          <div class='setting-body item'></div>
        </div>
      </div>
-->
      <div class='songlist' id='songlist'>
        <% music.forEach(function(name, index){ %>
        <div class='item' value='<%= index %>'>[<%= index+1 %>] <%= name %>
        </div>
        <% }); %>
      </div>
      <div class='effectlist' id='effectlist'>
        <div class='effect selected'>
          <span class="glyphicon glyphicon-cd" aria-hidden="true"></span>
        </div>
        <div class='effect'>
          <span class="glyphicon glyphicon-signal" aria-hidden="true"></span>
        </div>
        <div class='effect'>
          <span class="glyphicon glyphicon-repeat" aria-hidden="true"></span>
        </div>
        <div class='effect'>
          <span class="glyphicon glyphicon-record" aria-hidden="true"></span>
        </div>
        <div class='effect'>
          <span class="glyphicon glyphicon-refresh" aria-hidden="true"></span>
        </div>
        <div class='effect'>
          <span class="glyphicon glyphicon-fire" aria-hidden="true"></span>
        </div>
        <div class='effect'>
          <span class="glyphicon glyphicon-stats" aria-hidden="true"></span>
        </div>
        <div class='effect'>
          <span class="glyphicon glyphicon-fullscreen" aria-hidden="true"></span>
        </div>
        <div class='effect'>
          <span class="glyphicon glyphicon-star-empty" aria-hidden="true"></span>
        </div>
        <div class='effect'>
          <span class="glyphicon glyphicon-star" aria-hidden="true"></span>
        </div>
      </div>
      <div class='progressbar'>
        <div class="progressbar progressing" role="progressbar" aria-valuenow="80" aria-valuemin="0" aria-valuemax="100" style="width: 0%">
        </div>
      </div>
      <div class='footer'>
        <div class='container'>
          <div class='my-col'>
            <span class="glyphicon glyphicon-info-sign" aria-hidden="true"></span>
          </div>
          <div class='my-col'>
            <span class="glyphicon glyphicon-volume-down" aria-hidden="true"></span>
          </div>
          <div class='my-col'>
            <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
          </div>
          <div class='my-col'>
            <span class="glyphicon glyphicon-play" aria-hidden="true"></span>
          </div>
          <div class='my-col'>
            <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
          </div>
          <div class='my-col'>
            <span class="glyphicon glyphicon-volume-up" aria-hidden="true"></span>
          </div>
          <div class='my-col'>
            <span class="glyphicon glyphicon-menu-hamburger" aria-hidden="true"></span>
          </div>
        </div>
      </div>
      
      <div id='welcome' class='dis'>
        <div class='circle' id='welcome-cir'>
          <div id='welcome-text'>Start</div>
        </div>
      </div>
    </div>
    <script src='/javascripts/data.js'></script>
    <script src='/javascripts/index.js'></script>
    <script src='/javascripts/jquery-1.11.3.min.js'></script>
    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
<!--    <script src='http://cdn.bootcss.com/jquery/1.11.2/jquery.min.js'></script>-->
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src='/javascripts/bootstrap.min.js'></script>
  </body>
</html>
